{% for item in csvfile.split("\n") %}
{% if loop.index != 1 %}
{% set list = item.split("$") %}
vlan {{ list[1]|trim() }}
name "{{ list[0]|trim() }}"
{% if list[2] != "N/A" %}
ip address {{ list[2]|trim() }}
{% if list[3] != "N/A" %}
{% for each_vlan_ip_helper in list[3] %}
ip helper-address {{ each_vlan_ip_helper|trim() }}
{% endfor %}
{% endif %}
{% endif %}
{% endif %}
{% endfor %}
